{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "MentalNote - simple bookmarking and productivity tool",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        },
        "files": []
    },
    "require": {
        "php": "^8.2",
        "ext-memcached": "*",
        "babdev/pagerfanta-bundle": "^4.0",
        "doctrine/doctrine-bundle": "@stable",
        "doctrine/doctrine-migrations-bundle": "@stable",
        "doctrine/orm": "^2.6.0",
        "pagerfanta/doctrine-orm-adapter": "^4.2",
        "pagerfanta/pagerfanta": "^4.0",
        "psr/simple-cache": "^1.0",
        "sensio/framework-extra-bundle": "@stable",
        "symfony/asset": "^6.0",
        "symfony/browser-kit": "^6.0",
        "symfony/cache": "^6.0",
        "symfony/console": "^6.0",
        "symfony/flex": "^1.18",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/monolog-bundle": "@stable",
        "symfony/password-hasher": "^6.0",
        "symfony/runtime": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/security-csrf": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/yaml": "^6.0",
        "twig/extensions": "~1.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "@stable",
        "ergebnis/composer-normalize": "^2.34",
        "phpunit/phpunit": "^9.0",
        "symfony/dotenv": "^6.0",
        "symfony/maker-bundle": "^1.50",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "docker": false
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "bin-dir": "bin",
        "allow-plugins": {
            "symfony/flex": true,
            "ergebnis/composer-normalize": true,
            "symfony/runtime": true
        }
    }
}
